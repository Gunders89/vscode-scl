{
    "name": "vscode-siemens-scl",
    "displayName": "Siemens SCL",
    "description": "Siemens SCL language support for Visual Studio Code, based on gunders89 code",
    "icon": "images/sclIcon.png",
    "version": "0.2.0",
    "publisher": "sikilde",
    "engines": {
        "vscode": "^2.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sikilde/vscode-scl.git"
    },
    "homepage": "https://github.com/sikilde/vscode-scl/blob/master/README.md",
    "contributes": {
        "languages": [
            {
                "id": "siemensscl",
                "aliases": [
                    "Siemens SCL",
                    "SCL",
                    "scl"
                ],
                "extensions": [
                    "scl",
                    "SCL",
                    "udt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "siemensscl",
                "scopeName": "source.scl",
                "path": "./syntaxes/vscode-siemens-scl.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "siemensscl",
                "path": "./snippets/vscode-siemens-scl.json"
            }
        ]
    }
}
